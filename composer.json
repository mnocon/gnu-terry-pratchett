    "type": "library",
    "license": "MIT License",
    "authors": [
        {
            "name": "Marek Noco≈Ñ",
            "email": "markno@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^4.0 || 5.0.*"
    },
    "require-dev": {
        "behat/behat": "^3.6",
        "behat/mink": "^1.8",
        "friends-of-behat/mink-extension": "^2.4",
        "behat/mink-goutte-driver": "^1.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^0.12.14"

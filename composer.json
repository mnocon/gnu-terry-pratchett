{
    "name": "mnocon/gnu-terry-pratchett",
    "description": "Add X-Clacks-Overhead \"GNU Terry Pratchett\" to your website headers",
    "type": "library",
    "license": "MIT License",
    "authors": [
        {
            "name": "Marek Noco≈Ñ",
            "email": "markno@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "^4.0 || 5.0.*",
        "symfony/yaml": "^4.0 || 5.0.*"
    },
    "require-dev": {
        "behat/behat": "^3.6",
        "behat/mink": "^1.8",
        "behat/mink-goutte-driver": "^1.2",
        "dg/bypass-finals": "^1.1",
        "friends-of-behat/mink-extension": "^2.4",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpstan/phpstan": "^0.12.14",
        "phpunit/phpunit": "^9"
    },
    "config": {
        "sort-packages": true,
        "bin-dir": "bin/"
    },
    "autoload": {
        "psr-4": {
            "MNocon\\GnuTerryPratchettBundle\\": "src/bundle/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MNocon\\GnuTerryPratchett\\Tests\\": "Tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
